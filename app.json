{
  "expo": {
    "name": "DivideAI",
    "slug": "divideai-app-trae",
    "version": "1.0.0",
    "scheme": "divideai",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.alecgd.divideai",
      "buildNumber": "1",
      "infoPlist": {
        "NSCameraUsageDescription": "Permitir acesso à câmera para tirar fotos de comprovantes.",
        "NSPhotoLibraryUsageDescription": "Permitir acesso à biblioteca para selecionar imagens de comprovantes."
      }
    },
    "android": {
      "package": "com.alecgd.divideai",
      "versionCode": 1,
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      },
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "divideai",
              "host": "pro",
              "pathPrefix": "/"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    }
  }
}
